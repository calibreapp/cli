{
  "name": "calibre",
  "version": "4.2.0",
  "engines": {
    "node": ">= 12.22.4"
  },
  "description": "Calibre - Page speed performance testing with Google Lighthouse",
  "author": "calibreapp.com",
  "main": "index.js",
  "bin": {
    "calibre": "./src/cli.js"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "columnify": "^1.5.4",
    "configstore": "^5.0.0",
    "cookiefile": "^1.0.10",
    "date-fns": "^2.9.0",
    "graphql-request": "^1.4.1",
    "humanize": "0.0.9",
    "json2csv": "^5.0.0",
    "listr": "^0.14.1",
    "log-symbols": "^4.0.0",
    "ora": "^5.1.0",
    "stats-percentile": "^3.1.0",
    "update-notifier": "^5.0.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-plugin-node": "^11.0.0",
    "execa": "^5.0.0",
    "express": "^4.16.4",
    "jest": "^27.0.6",
    "pkg": "5.5.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "ci": "CI=true TZ=Etc/GMT jest --coverage --runInBand --no-colors",
    "test": "TZ=Etc/GMT jest --runInBand --no-colors",
    "lint": "eslint .",
    "package": "pkg . --out-path=bin",
    "pre-publish": "echo //registry.npmjs.org/:_authToken=${NPM_AUTH_TOKEN} > .npmrc"
  },
  "keywords": [
    "performance",
    "testing",
    "test",
    "lighthouse"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/calibreapp/cli.git"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "pkg": {
    "scripts": [
      "src/**/*.js"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ]
  }
}
