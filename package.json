    "date-fns": "^1.28.5",
    "graphql-request": "^1.4.1",
    "humanize": "0.0.9",
    "json2csv": "^3.11.5",
    "listr": "^0.14.1",
    "ora": "^3.4.0",
    "stats-percentile": "^3.1.0",
    "update-notifier": "^3.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-plugin-node": "^9.1.0",
    "execa": "^2.0.3",
    "express": "^4.16.4",
    "jest": "^24.7.1",
    "pkg": "4.3.7",
